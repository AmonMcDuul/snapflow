{
  "name": "flowrender",
  "version": "1.0.0",
  "description": "A lightweight TypeScript library for rendering clean SVG flowcharts with zoom and pan support.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "flowchart",
    "svg",
    "typescript",
    "diagram",
    "visualization"
  ],
  "author": "AmonMcDuul",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "@types/dagre": "^0.7.47",
    "dagre": "^0.8.5"
  },
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/AmonMcDuul/flowrender/"
  }
}
